          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="reonet_MonthlyChartTableAdapter" GeneratorDataComponentClassName="reonet_MonthlyChartTableAdapter" Name="reonet_MonthlyChart" UserDataComponentName="reonet_MonthlyChartTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="reonetfi_dbConnectionString (MySettings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
SELECT m.MonthNumber, m.MonthName, ISNULL(t.TotalPrice, 0) AS TotalPrice, t.companyname
FROM (
    SELECT 1 AS MonthNumber, 'January' AS MonthName UNION ALL
    SELECT 2, 'February' UNION ALL
    SELECT 3, 'March' UNION ALL
    SELECT 4, 'April' UNION ALL
    SELECT 5, 'May' UNION ALL
    SELECT 6, 'June' UNION ALL
    SELECT 7, 'July' UNION ALL
    SELECT 8, 'August' UNION ALL
    SELECT 9, 'September' UNION ALL
    SELECT 10, 'October' UNION ALL
    SELECT 11, 'November' UNION ALL
    SELECT 12, 'December'
) AS m
LEFT JOIN (
    SELECT rc.companyname, MONTH(om.OrderDate) AS MonthNumber, SUM(od.TotalPrice) AS TotalPrice
    FROM reonet_ordermaster om
    JOIN reonet_orderdetail od ON om.srl = od.srl_ordermaster
    JOIN reonet_customer rc ON rc.srl = om.srl_customer
    GROUP BY MONTH(om.OrderDate), rc.companyname
) AS t ON m.MonthNumber = t.MonthNumber
WHERE m.MonthNumber &lt;= MONTH(GETDATE())</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
